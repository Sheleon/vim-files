-module(hss_store).

-behaviour(application).

%% Application callbacks
-export([start/0, start/2, stop/1]).

start() -> 
    application:start(inets),
    application:start(hss_store).

%% ===================================================================
%% Application callbacks
%% ===================================================================

start(_StartType, _StartArgs) ->
    error_logger:info_msg("Starting hss_store application...~n"),
    hss_store_sup:start_link().

stop(_State) ->
    ok.
