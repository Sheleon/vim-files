%% @doc Callbacks for the wac application.
-module(wac_app).

-behaviour(application).
-export([start/2,stop/1]).


%% @spec start(_Type, _StartArgs) -> ServerRet
%% @doc application start callback for wac.
start(_Type, _StartArgs) ->
    wac_sup:start_link().

%% @spec stop(_State) -> ServerRet
%% @doc application stop callback for wac.
stop(_State) ->
    ok.
