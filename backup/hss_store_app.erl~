-module(hss_store_app).

-behaviour(application).

%% Application callbacks
-export([start/2, stop/1]).

start() -> application:start(hss_store).

%% ===================================================================
%% Application callbacks
%% ===================================================================

start(_StartType, _StartArgs) ->
    error_logger:info_msg("Starting hs_store application...~n"),
    hss_store_sup:start_link().

stop(_State) ->
    ok.
