-module(ole_replicant).
-behaviour(application).

-export([start/0, start/2, stop/1]).

start() -> 
    application:start(ole_replicant).

start(_StartType, _StartArgs) ->
    error_logger:info_msg("Starting ole_replicant application...~n"),
    ole_replicant_sup:start_link().

stop(_State) ->
    ok.
