-module(core_four_oh_four).
-behaviour(cowboy_http_handler).
-export([test/0, init/3, handle/2, terminate/2]).

init({_Any, http}, Req, []) ->
    {ok, Req, undefined}.

handle(Req, State) ->
    {ok, Reply} = cowboy_http_req:reply(404, [], <<"Ack! Couldn't find it.">>, 
        Req),
    {ok, Reply, State}.

terminate(_Req, _State) ->
    ok.

test() ->
    ok.
